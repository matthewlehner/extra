{
  "moduleFileExtensions": [
    "js",
    "jsx"
  ],
  "moduleDirectories": [
    "node_modules",
    "web/static/js"
  ],
  "moduleNameMapper": {
    "\\.(css|scss|svg)$": "identity-obj-proxy"
  },
  "transform": {
    "\\.(gql|graphql)$": "jest-transform-graphql",
    "\\.(js|jsx)$": "babel-jest"
  }
}
